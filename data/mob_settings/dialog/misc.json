  "action": {
    "label": {
      "translate": "menu.mob_settings.option.confirm",
      "fallback": "Confirm"
    },
    "id": "confirm",
    "on_submit": {
      "type": "minecraft:command_template",
      "template": "function mob_settings:menu_config/misc {immunezombie:$(immunezombie),creeperfuse:$(creeperfuse),skeletonhorsetrap:$(skeletonhorsetrap),irongolemanger:$(irongolemanger),brownmoospawning:$(brownmoospawning),illusionerspawning:$(illusionerspawning),babymountspawning:$(babymountspawning),killerrabbitspawning:$(killerrabbitspawning),jebcolor:$(jebcolor),jebspawning:$(jebspawning),egglay:$(egglay),need_sky:$(need_sky)}"
    }
  }
