{
  "type": "minecraft:simple_input_form",
  "body": [
    {
      "type": "minecraft:item",
      "item": {
        "id": "minecraft:spawner",
        "components": {
          "minecraft:tooltip_display": {
            "hide_tooltip": true
          }
        }
      },
      "description": {
        "translate": "menu.mob_settings.passive_selection.description",
        "fallback": "Configure Miscellaneous Settings:"
      }
    },
    {
      "type": "minecraft:plain_message",
      "contents": {
        "translate": "menu.mob_settings.init_values",
        "fallback": "Initial values are always default values.",
        "color": "gray"
      },
      "width": 1024
    }
  ],
  "can_close_with_escape": true,
  "external_title": {
    "translate": "menu.mob_settings.misc_settings.title",
    "fallback": "Miscellaneous"
  },
  "title": {
    "translate": "menu.mob_settings.main.title",
    "fallback": "Nice Mob Settings"
  },
  "inputs": [
    {
      "type": "minecraft:single_option",
      "key": "cmdfeedback",
      "label": {
        "translate": "menu.mob_settings.option.commandfeedback",
        "fallback": "Send Command Feedback"
      },
      "options": [
        {
          "id": "gamerule sendCommandFeedback true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule sendCommandFeedback false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "need_sky",
      "label": {
        "translate": "menu.mob_settings.option.need_sky",
        "fallback": "Scaling only on Surface"
      },
      "options": [
        {
          "id": "enabled",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "disabled",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "insomnia",
      "label": {
        "translate": "menu.mob_settings.option.insomnia",
        "fallback": "Phantom Spawning"
      },
      "options": [
        {
          "id": "gamerule doInsomnia true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule doInsomnia false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "warden",
      "label": {
        "translate": "menu.mob_settings.option.warden",
        "fallback": "Warden Spawning"
      },
      "options": [
        {
          "id": "gamerule doWardenSpawning true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule doWardenSpawning false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "egglay",
      "label": {
        "translate": "menu.mob_settings.option.egglay",
        "fallback": "Chickens lay Eggs"
      },
      "options": [
        {
          "id": "enabled",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "disabled",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "raids",
      "label": {
        "translate": "menu.mob_settings.option.raids",
        "fallback": "Raids"
      },
      "options": [
        {
          "id": "gamerule disableRaids true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule disableRaids false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "mobloot",
      "label": {
        "translate": "menu.mob_settings.option.mobloot",
        "fallback": "Mob Loot"
      },
      "options": [
        {
          "id": "gamerule doMobLoot true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule doMobLoot false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "patrols",
      "label": {
        "translate": "menu.mob_settings.option.patrols",
        "fallback": "Patrol Spawning"
      },
      "options": [
        {
          "id": "gamerule doPatrolSpawning true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule doPatrolSpawning false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "trader",
      "label": {
        "translate": "menu.mob_settings.option.trader",
        "fallback": "Wandering Trader Spawning"
      },
      "options": [
        {
          "id": "gamerule doTraderSpawning true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule doTraderSpawning false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:single_option",
      "key": "griefing",
      "label": {
        "translate": "menu.mob_settings.option.griefing",
        "fallback": "Mob Griefing"
      },
      "options": [
        {
          "id": "gamerule mobGriefing true",
          "display": {
            "translate": "menu.nice_mobs.enabled",
            "fallback": "Enabled",
            "color": "green"
          },
          "initial": true
        },
        {
          "id": "gamerule mobGriefing false",
          "display": {
            "translate": "menu.nice_mobs.disabled",
            "fallback": "Disabled",
            "color": "red"
          }
        }
      ]
    },
    {
      "type": "minecraft:number_range",
      "key": "cramming",
      "label": {
        "translate": "menu.mob_settings.option.cramming",
        "fallback": "Max Entity Cramming"
      },
      "start": 1,
      "end": 1024,
      "step": 1,
      "initial": 24
    }
  ],
  "action": {
    "label": {
      "translate": "menu.mob_settings.option.confirm",
      "fallback": "Confirm"
    },
    "id": "confirm",
    "on_submit": {
      "type": "minecraft:command_template",
      "template": "function mob_settings:menu_config/misc {cramming:$(cramming),griefing:\"$(griefing)\",trader:\"$(trader)\",patrols:\"$(patrols)\",mobloot:\"$(mobloot)\",raids:\"$(raids)\",egglay:$(egglay),need_sky:$(need_sky),insomnia:\"$(insomnia)\",warden:\"$(warden)\",cmdfeedback:\"$(cmdfeedback)\"}"
    }
  }
}