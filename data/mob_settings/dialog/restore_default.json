{
  "type": "minecraft:confirmation",
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": {
        "translate": "menu.mob_settings.restore.description",
        "fallback": "Proceeding will reset all set values. Do you want to continue?"
      },
      "width": 1024
    },
    {
      "type": "minecraft:plain_message",
      "contents": {
        "translate": "menu.mob_settings.restore.warning",
        "fallback": "Restoring default values will not affect already existing mobs!",
        "color": "gray"
      },
      "width": 1024
    }
  ],
  "can_close_with_escape": true,
  "external_title": {
    "translate": "menu.mob_settings.restore.title",
    "fallback": "Restore Default Values"
  },
  "title": {
    "translate": "menu.mob_settings.main.title",
    "fallback": "Nice Mob Settings"
  },
  "yes": {
    "label": {
      "translate": "menu.mob_settings.confirm",
      "fallback": "Confirm"
    },
    "on_click": {
      "action": "run_command",
      "command": "function mob_settings:menu_config/restore_default"
    }
  },
  "no": {
    "label": {
      "translate": "menu.mob_settings.back",
      "fallback": "Back"
    },
    "on_click": {
      "action": "show_dialog",
      "dialog": "mob_settings:main"
    }
  }
}