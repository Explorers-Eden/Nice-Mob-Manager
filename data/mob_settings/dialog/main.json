{
  "type": "minecraft:multi_action",
  "body": {
    "type": "minecraft:item",
    "item": {
      "id": "minecraft:spawner",
      "components": {
        "minecraft:tooltip_display": {
          "hide_tooltip": true
        }
      }
    },
    "description": {
      "translate": "menu.mob_settings.main.description",
      "fallback": "Select a Mob Category:"
    }
  },
  "can_close_with_escape": true,
  "external_title": {
    "translate": "menu.mob_settings.main.title",
    "fallback": "Nice Mob Settings"
  },
  "title": {
    "translate": "menu.mob_settings.main.title",
    "fallback": "Nice Mob Settings"
  },
  "pause": true,
  "exit_action": {
    "label": {
      "translate": "mco.selectServer.close"
    }
  },
  "columns": 1,
  "actions": [
    {
      "label": {
        "translate": "menu.mob_settings.passive_mobs.title",
        "fallback": "Passive Mobs"
      },
      "action": {
        "type": "minecraft:show_dialog",
        "dialog": "mob_settings:passive"
      }
    },
    {
      "label": {
        "translate": "menu.mob_settings.neutral_mobs.title",
        "fallback": "Neutral Mobs"
      },
      "action": {
        "type": "minecraft:show_dialog",
        "dialog": "mob_settings:neutral"
      }
    },
    {
      "label": {
        "translate": "menu.mob_settings.hostile_mobs.title",
        "fallback": "Hostile Mobs"
      },
      "action": {
        "type": "minecraft:show_dialog",
        "dialog": "mob_settings:hostile"
      }
    },
    {
      "label": {
        "translate": "menu.mob_settings.misc_settings.title",
        "fallback": "Miscellaneous"
      },
      "action": {
        "type": "minecraft:run_command",
        "command": "function mob_settings:dialog/dynamic/misc with storage eden:settings mob_settings.misc"
      }
    },
    {
      "label": {
        "translate": "menu.mob_settings.vanilla_gamerules",
        "fallback": "Vanilla Gamerules"
      },
      "action": {
        "type": "minecraft:run_command",
        "command": "function mob_settings:dialog/dynamic/gamerules with storage eden:settings mob_settings.gamerules"
      }
    },
    {
      "label": {
        "translate": "menu.mob_settings.restore.title",
        "fallback": "Restore Default Values"
      },
      "action": {
        "type": "minecraft:show_dialog",
        "dialog": "mob_settings:restore_default"
      }
    },
    {
      "label": {
        "translate": "menu.eden.info.title",
        "fallback": "Explorer's Eden"
      },
      "action": {
        "type": "minecraft:show_dialog",
        "dialog": "eden:info"
      }
    }
  ]
}