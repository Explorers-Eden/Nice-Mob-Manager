    {
      "type": "minecraft:number_range",
      "key": "immunezombie",
      "label": {
        "translate": "menu.mob_settings.option.immunezombie",
        "fallback": "Immune To Zombification Chance"
      },
      "label_format": "options.percent_value",
      "start": 0,
      "end": 100,
      "step": 1,
      "initial": 0
    },
    {
      "type": "minecraft:number_range",
      "key": "brownmoospawning",
      "label": {
        "translate": "menu.mob_settings.option.brownmoospawning",
        "fallback": "Brown Mooshroom Spawn Chance"
      },
      "label_format": "options.percent_value",
      "start": 0,
      "end": 100,
      "step": 1,
      "initial": 15
    }
